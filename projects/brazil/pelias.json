{
  "logger": {
    "level": "debug",
    "timestamp": false
  },
  "dbclient": {
    "apiVersion": "7.5",
    "hosts": [
      {
        "protocol": "http",
        "host": "host.docker.internal",
        "port": 9200
      }
    ],
    "engine": "opensearch"
  },
  "opensearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "3"
      }
    }
  },
  "acceptance-tests": {
    "endpoints": {
      "docker": "http://api:4000/v1/"
    }
  },
  "api": {
    "services": {
      "placeholder": {
        "url": "http://placeholder:4100"
      },
      "pip": {
        "url": "http://pip:4200"
      },
      "interpolation": {
        "url": "http://interpolation:4300"
      },
      "libpostal": {
        "url": "http://libpostal:4400"
      }
    }
  },
  "imports": {
    "adminLookup": {
      "enabled": true
    },
    "geonames": {
      "datapath": "/data/geonames",
      "countryCode": "BR"
    },
    "openstreetmap": {
      "download": [
        {
          "sourceURL": "https://data.geocode.earth/archive/portland-metro/2022-06-20/portland_oregon.osm.pbf"
        }
      ],
      "leveldbpath": "/tmp",
      "datapath": "/data/openstreetmap",
      "import": [
        {
          "filename": "portland_oregon.osm.pbf"
        }
      ]
    },
    "openaddresses": {
      "datapath": "/data/openaddresses",
      "files": [
        "br/ac/statewide",
        "br/al/statewide",
        "br/am/statewide",
        "br/ap/statewide",
        "br/ba/statewide",
        "br/ce/statewide",
        "br/df/statewide",
        "br/es/statewide",
        "br/go/statewide",
        "br/ma/statewide",
        "br/mg/statewide",
        "br/ms/statewide",
        "br/mt/statewide",
        "br/pa/statewide",
        "br/pb/statewide",
        "br/pe/statewide",
        "br/pi/statewide",
        "br/pr/statewide",
        "br/rj/statewide",
        "br/rn/statewide",
        "br/ro/statewide",
        "br/rr/statewide",
        "br/rs/statewide",
        "br/sc/statewide",
        "br/se/statewide",
        "br/sp/statewide",
        "br/to/statewide",
        "us/or/portland_metro",
        "us/or/city_of_salem"
      ]
    },
    "polyline": {
      "datapath": "/data/polylines",
      "files": [
        "extract.0sv"
      ]
    },
    "whosonfirst": {
      "datapath": "/data/whosonfirst",
      "countryCode": "BR",
      "importPostalcodes": true,
      "importPlace": [
        "85688513",
        "85688623"
      ]
    }
  }
}
