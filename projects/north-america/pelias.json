{
  "logger": {
    "level": "info",
    "timestamp": false
  },
  "dbclient": {
    "hosts": [
      {
        "protocol": "http",
        "host": "opensearch",
        "port": 9200
      }
    ],
    "engine": "opensearch"
  },
  "opensearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "4"
      }
    }
  },
  "acceptance-tests": {
    "endpoints": {
      "docker": "http://api:4000/v1/"
    }
  },
  "api": {
    "services": {
      "pip": {
        "url": "http://pip:4200",
        "maxConcurrentRequests": 24
      },
      "wof-admin-lookup": {
        "maxConcurrentRequests": 24
      },
      "libpostal": {
        "url": "http://libpostal:4400"
      },
      "placeholder": {
        "url": "http://placeholder:4100"
      },
      "interpolation": {
        "url": "http://interpolation:4300"
      }
    }
  },
  "imports": {
    "adminLookup": {
      "maxConcurrent": 24,
      "enabled": true
    },
    "csv": {
      "datapath": "/data/csv",
      "files": [],
      "download": []
    },
    "geonames": {
      "datapath": "/data/geonames",
      "countryCode": "US"
    },
    "openstreetmap": {
      "download": [
        {
          "sourceURL": "https://download.geofabrik.de/north-america/us-latest.osm.pbf"
        }
      ],
      "leveldbpath": "/data/openstreetmap/leveldb",
      "datapath": "/data/openstreetmap",
      "import": [
        {
          "filename": "us-latest.osm.pbf"
        }
      ]
    },
    "openaddresses": {
      "datapath": "/data/openaddresses",
      "files": [
        "us"
      ]
    },
    "polyline": {
      "datapath": "/data/polylines",
      "files": [
        "extract.0sv"
      ]
    },
    "whosonfirst": {
      "datapath": "/data/whosonfirst",
      "importPostalcodes": true,
      "importPlace": "85633793"
    },
    "interpolation": {
      "download": {
        "tiger": {
          "datapath": "/data/tiger",
          "states": "all"
        }
      }
    }
  }
}